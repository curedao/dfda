  "scripts": {
    "cy:open": "cypress open",
    "cy:serve:open": "cross-env-shell APP_ENV=localhost npm-run-all --parallel dev cy:open",
    "start": "node index.js",
    "dev": "npx nodemon index.js -e js,ejs,html,css",
    "generate": "npx prisma generate",
    "start-server-and-test": "node tests/start-server-and-test.js",
    "start-server-and-cy-run": "cross-env-shell EXPRESS_ORIGIN=http://localhost:5000 npm-run-all --parallel start cy:run",
    "start-server-and-cy-open": "start-server-and-test 'http-server public -c-1 -p 5000 --silent' 5000 'cypress open'",
    "http-server": "http-server -p 5000 public -a localhost http://localhost:5000 -o -c-1",
    "cy:run": "cypress run",
    "install:global:packages": "nvm install 16.13.0 && nvm use 16.13.0",
    "cy:open:ionic": "cross-env OAUTH_APP_ORIGIN='https://medimodo.herokuapp.com' cypress open",
    "ci": "npm run test",
    "circle": "npm run lint && npm run test:ci && make test && npm run pre-publish",
    "compile": "npm run types",
    "configure:app": "npm run types:ts && node ts/qm.builder.js",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test",
    "cy:chrome": "cypress run --browser chrome",
    "cy:record": "cypress run --record",
    "cy:record:parallel": "cypress run --record --parallel",
    "cy:runner": "npx ts-node cypress/cypress-runner.ts",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "deploy:prepare": "./scripts/create_npmrc_file.sh",
    "git:pre:commit": "npm run lint:fix && npm run types && npm run test:mocha",
    "git:delete-feature-branches": "git branch | grep \"/feature\" | xargs git branch -D",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:yaml",
    "lint:es": "eslint .",
    "lint:es-fix": "eslint . --fix",
    "lint:fix": "npm run lint:es-fix && npm run lint:ts-fix",
    "lint:ts": "cd ts && tslint --project .",
    "lint:ts-fix": "cd ts && tslint --project . --fix",
    "lint:yaml": "yamllint '*.yml' 'basic/*.yml' .buildkite/pipeline.yml .semaphore/semaphore.yml '.github/workflows/*.yml'",
    "preinstall": "echo \"STARTING npm install\"",
    "postinstall": "echo \"DONE WITH npm install\" && npm run generate && npm run types",
    "print-env": "print-env",
    "pull-develop": "bash scripts/git-pull-develop.sh",
    "ship-it": "npm run pre-publish && npm publish",
    "api": "node api.js",
    "start:ci": "serve --no-clipboard --listen ${PORT:-8080}",
    "test": "npm run test:mocha && node tests/start-server-and-test.js && npm run test:ghostinspector",
    "test:ghostinspector": "npx ts-node ts/gi-runner.ts",
    "test:mocha": "mocha tests/mocha/**/*.test.js",
    "test:mocha:unit": "mocha tests/mocha/unit.mocha.test.js",
    "test:mocha:integration": "mocha tests/mocha/integration.mocha.test.js",
    "test:serve:mocha": "cross-env-shell EXPRESS_ORIGIN=http://localhost:5000 npm-run-all --parallel start test:mocha",
    "types": "npm run types:cypress && npm run types:ts",
    "types:cypress": "cd cypress && tsc -p ./tsconfig.json",
    "types:ts": "cd ts && tsc -p ./tsconfig.json",
    "up": "docker-compose up --abort-on-container-exit --exit-code-from e2e",
    "watch": "tsc --watch --noEmit --project ./ts/tsconfig.json",
    "openapi": "npx openapi-typescript-codegen --input ./docs/open-api-v3.yaml --output public/api"
  },
  "resolutions": {
    "set-value": "2.0.1",
    "braces": "2.3.2",
    "handlebars": "4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "exit 0 && cd ts && tsc --noEmit",
      "pre-push": ""
    }
  }
